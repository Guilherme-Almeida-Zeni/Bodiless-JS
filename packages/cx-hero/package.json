{
  "name": "@bodiless/cx-hero",
  "sideEffects": false,
  "version": "1.0.0-beta.1",
  "description": "Clean, designable Hero component for CanvasX based projects",
  "author": "Lucas Varela <lvarelab@its.jnj.com>",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/assets",
    "/doc"
  ],
  "repository": {
    "type": "git",
    "url": "https://sourcecode.jnj.com/scm/asx-nbfp/canvasx.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api ./src",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@bodiless/cx-elements": "^1.0.0-beta.1",
    "@bodiless/fclasses": "^1.0.0-beta.1",
    "@bodiless/hydration": "^1.0.0-beta.1"
  }
}
